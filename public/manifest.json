{
  "manifest_version": 3,
  "name": "Chrome Ebay Extension",
  "version": "1.1",  
  "description": "...",
  "permissions": ["identity", "storage"], 
  "host_permissions": ["https://www.googleapis.com/*", "https://docs.google.com/spreadsheets/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html" 
  },
  "content_security_policy": {
    "script-src": "'self' https://apis.google.com",
    "object-src": "'self'"
  }
}